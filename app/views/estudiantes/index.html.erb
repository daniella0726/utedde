          <% total = total + item %>
      <%end%>
      <% array = [] %>
      <% Estudiante.group(:p2).having("p2 != ?", "--").having("p2 != ?", "").order('COUNT(id) DESC').count(:id).each do |value,item|%>
          <%= value%>
          <% valor = "" %>
          <% if (value = "1") %>
            <%valor = "Femenino"%>
          <% else %>
            <%valor = "Masculino"%>
          <% end %>
          <% array.push([valor, (100*item/total.to_f).round(2)]) %>   
      <%end%>

      <%= bar_chart array, colors: ["#E84E0F"] , suffix: "%" %>

      <br>
    </div>

