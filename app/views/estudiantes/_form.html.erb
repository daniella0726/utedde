        event.preventDefault();
        document.getElementById("but_res_p60").click();
      }
    });

  document.getElementById("pregunta_pregunta_0_p60_boton").addEventListener("click", fun_p60_dev);
  function fun_p60_dev() {
      $('#div_pregunta_0').slideDown();
      $('#div_pregunta_0').removeClass('hidden');
      $('#pregunta_pregunta_0_footer').removeClass('hidden');
      $('#div_p60').slideUp();
      $('#div_p60').addClass('hidden');
      $('#pregunta_p60_footer').addClass('hidden');
      $('#div_error_p60').addClass('hidden');
  }

  document.getElementById("pregunta_p1_p60_boton").addEventListener("click", fun_p60);
  document.getElementById("but_res_p60").addEventListener("click", fun_p60);
    function fun_p60() {
    var frase = document.getElementById("res_p60").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_p60").value = frase;
    if (document.getElementById("res_p60").value == "") {
      $('#but_res_p60').addClass('hidden');
      $('#p_p60').addClass('hidden');
      $('#div_error_p60').removeClass('hidden');
    } else {
      $('#div_p60').slideUp();
      $('#div_p60').addClass('hidden');
      $('#pregunta_p60_footer').addClass('hidden');
      $('#div_p1').slideDown();
      $('#div_p1').removeClass('hidden');
      $('#pregunta_p1_footer').removeClass('hidden');
      $('#div_error_p1').addClass('hidden');
      document.getElementById("res_p1").focus();
    }
  }

  $(document).ready(function(){ 

    $('#res_p60').on('input', function() { 
      var frase = document.getElementById("res_p60").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      document.getElementById("res_p60").value = frase;
      if (document.getElementById("res_p60").value == "") {
      $('#but_res_p60').addClass('hidden');
      $('#p_p60').addClass('hidden');
      $('#div_error_p60').removeClass('hidden');
      } else {
       $('#but_res_p60').removeClass('hidden');
       $('#p_p60').removeClass('hidden');
       $('#div_error_p60').addClass('hidden');
      }
    }); 
  }); 
</script>

