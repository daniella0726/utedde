<style>
@import url('https://fonts.googleapis.com/css?family=Heebo:300,400,500,700&display=swap');
.hidden {
  display: none !important;}
  .centrado{
  position: absolute;
  top: 50%; 
  left: 50%;
  transform: translate(-50%, -50%);
  height: auto;
  }

  .centrado-abajo{
  position: absolute;
