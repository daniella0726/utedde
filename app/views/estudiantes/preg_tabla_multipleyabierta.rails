      input1.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("res_kaka5").focus();
      }
    });

  var input1 = document.getElementById("res_kaka5");
      input1.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("res_kaka6").focus();
      }
    });

  var input1 = document.getElementById("res_kaka6");
      input1.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("res_kaka7").focus();
      }
    });

