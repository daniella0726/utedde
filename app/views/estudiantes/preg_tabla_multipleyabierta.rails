          </div>
      </div>
      <div class="col-md-4">
        <div class="row">
          <a class="boton1" id="pregunta_voy_estoy_boton">
            <i class="fas fa-caret-square-down fa-3x" style="color: #254064; margin: 10px;"></i>
          </a>
          <a class="boton1" id="pregunta_antes_estoy_boton">
            <i class="fas fa-caret-square-up fa-3x" style="color: #254064; margin: 10px;"></i>
          </a>
        </div>
      </div>  
    </div>
  </div> 
</div>

<script type="text/javascript">

  document.getElementById("pregunta_antes_estoy_boton").addEventListener("click", fun_estoy_dev);
  function fun_estoy_dev() {
      $('#div_antes').slideDown();
      $('#div_antes').removeClass('hidden');
      $('#pregunta_antes_footer').removeClass('hidden');
      $('#div_estoy').slideUp();
      $('#div_estoy').addClass('hidden');
      $('#pregunta_estoy_footer').addClass('hidden');
      $('#div_error_estoy').addClass('hidden');
  }

  document.getElementById("pregunta_voy_estoy_boton").addEventListener("click", fun_estoy);
  document.getElementById("but_res_estoy").addEventListener("click", fun_estoy);


  document.getElementById("radio-1-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-2-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-3-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-4-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-5-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-6-estoy_1").addEventListener("click", fun_multi);
  document.getElementById("radio-7-estoy_1").addEventListener("click", fun_multi);

  function fun_multi() {
      $('#but_res_estoy').removeClass('hidden');
      $('#div_error_estoy').addClass('hidden');

  }

  function fun_estoy() {
    var frase = document.getElementById("res_kaka1").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_kaka1").value = frase;
    var frase1 = document.getElementById("res_kaka2").value; 
      frase1 = frase1.replace("#"," ");
      frase1 = frase1.replace("%"," ");
      frase1 = frase1.replace("&"," ");
      frase1 = frase1.replace("$"," ");
      frase1 = frase1.replace("  "," ");
      document.getElementById("res_kaka2").value = frase1;
    var frase2 = document.getElementById("res_kaka3").value; 
      frase2 = frase2.replace("#"," ");
      frase2 = frase2.replace("%"," ");
      frase2 = frase2.replace("&"," ");
      frase2 = frase2.replace("$"," ");
      frase2 = frase2.replace("  "," ");
      document.getElementById("res_kaka3").value = frase2;

    if (document.getElementById("res_kaka1").value == "" ||
      document.getElementById("res_kaka2").value == "" ||
      document.getElementById("res_kaka3").value == "" ||
      document.getElementById("res_kaka4").value == "" ||
      document.getElementById("res_kaka5").value == "" ||
      document.getElementById("res_kaka6").value == "" ||
      document.getElementById("res_kaka7").value == "" ||
      $("#radio-1-estoy_1").is(':checked') == true || 
        $("#radio-2-estoy_2").is(':checked') == true || 
        $("#radio-3-estoy_3").is(':checked') == true || 
        $("#radio-4-estoy_4").is(':checked') == true || 
        $("#radio-5-estoy_5").is(':checked') == true || 
        $("#radio-6-estoy_6").is(':checked') == true || 
        $("#radio-7-estoy_7").is(':checked') == true) {

    setTimeout(function(){
      $('#div_estoy').slideUp();
      $('#div_estoy').addClass('hidden');
      $('#pregunta_estoy_footer').addClass('hidden');
      $('#div_voy').slideDown();
      $('#div_voy').removeClass('hidden');
      $('#pregunta_voy_footer').removeClass('hidden');
      $('#div_error_voy').addClass('hidden');
  } , 500);
  } else {
      $('#div_error_estoy').removeClass('hidden');
    }
    }

  var input = document.getElementById("res_kaka1");
      input.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("res_kaka2").focus();
      }
    });

  var input1 = document.getElementById("res_kaka2");
      input1.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("res_kaka3").focus();
      }
    });

