<%= form_with(model: director, local: true) do |f| %>
  <% if director.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(director.errors.count, "error") %> prohibited this director from being saved:</h2>

      <ul>
      <% director.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

<div class="container" style="text-align: center; max-width: 1400px;">

<div class="centrado" style="width: 90%; text-align: center; position: absolute;" id="div_pregunta_0">
  <h2 style="font-family: 'Heebo', sans-serif; color: #254064;">Empezar nueva encuesta directivos</h2>
  <button type="button" class="boton3" style="" id="but_res_pregunta_0">
    <i class="fas fa-check-circle"; id="but_pregunta_0"></i>
    Empieza!
  </button>
</div>

<script type="text/javascript">
  $('form').on('keypress', e => {
    if (e.keyCode == 13) {
        return false;
    }
  });
  var input = document.getElementById("but_res_pregunta_0");
      input.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("but_res_pregunta_0").click();
      }
    });
  document.getElementById("but_res_pregunta_0").addEventListener("click", fun_pregunta_0);
  function fun_pregunta_0() {
      $('#div_pregunta_0').slideUp();
      $('#div_pregunta_0').addClass('hidden');
      $('#div_p1').slideDown();
      $('#div_p1').removeClass('hidden');
      $('#pregunta_1_footer').removeClass('hidden');
      $('#div_error_1').addClass('hidden');
  }

</script>

<div class="centrado hidden" style="width: 90% position: absolute!important;" id="div_p1">
  <h2 style="font-weight: bold;font-family: 'Heebo', sans-serif; color: #254064;">P1. ¿Eres hombre o mujer? </h2>
  <br>
  <div class="field qts" >
    <div class="inputGroup">
      <%= f.radio_button :p1, "1" , id:"radio-1-p1"%>
      <%= f.label :p1, "Hombre", for: "radio-1-p1"%>
    </div>
    <div class="inputGroup">
      <%= f.radio_button :p1, "2" , id:"radio-2-p1"%>
      <%= f.label :p1, "Mujer", for: "radio-2-p1"%>
    </div>
  </div>
  <div class="invalid-feedback hidden" style="color: #4c2f48;" id="div_error_pregunta_1">Por favor, <strong>escogé un curso!</strong></div>
  <button type="button" class="boton3 hidden" id="but_res_pregunta_1">
    <i class="fas fa-check-circle"></i>
      OK
  </button>
  <p  class="hidden" style="color: #4c2f48;" id="p_p1">presioná <strong>ENTER</strong></p>
</div>  
<div class="container hidden" id="pregunta_p1_footer">
  <div class="centrado-abajo">
    <div class="row">
      <div class="col-md-4">
        <span style="color: #254064">1 de 32 respuestas</span>
