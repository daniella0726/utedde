            <i class="fas fa-caret-square-up fa-3x" style="color: #254064; margin: 10px;"></i>
          </a>
        </div>
      </div>  
    </div>
  </div> 
</div>

<script type="text/javascript">

  document.getElementById("pregunta_0_p1_boton").addEventListener("click", fun_p1_dev);
  function fun_p1_dev() {
      $('#div_pregunta_0').slideDown();
      $('#div_pregunta_0').removeClass('hidden');
      $('#pregunta_0_footer').removeClass('hidden');
      $('#div_p1').slideUp();
      $('#div_p1').addClass('hidden');
      $('#pregunta_p1_footer').addClass('hidden');
      $('#div_error_p1').addClass('hidden');
  }

  document.getElementById("pregunta_p2_p1_boton").addEventListener("click", fun_p1);
  document.getElementById("radio-1-p1").addEventListener("click", fun_p1);
  document.getElementById("radio-2-p1").addEventListener("click", fun_p1);
  function fun_p1() {
    if ($("#radio-1-p1").is(':checked') == true || 
        $("#radio-2-p1").is(':checked') == true ) {
    setTimeout(function(){
      $('#div_p1').slideUp();
      $('#div_p1').addClass('hidden');
      $('#pregunta_p1_footer').addClass('hidden');
      $('#div_p2').slideDown();
      $('#div_p2').removeClass('hidden');
      $('#pregunta_p2_footer').removeClass('hidden');
      $('#div_error_p2').addClass('hidden');
  } , 500);
  } else {
      $('#div_error_p1').removeClass('hidden');
    }
    }
</script>

