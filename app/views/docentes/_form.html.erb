      $('#div_pregunta_0').slideDown();
      $('#div_pregunta_0').removeClass('hidden');
      $('#pregunta_pregunta_0_footer').removeClass('hidden');
      $('#div_pen').slideUp();
      $('#div_pen').addClass('hidden');
      $('#pregunta_pen_footer').addClass('hidden');
      $('#div_error_pen').addClass('hidden');
  }

  document.getElementById("pregunta_p1_pen_boton").addEventListener("click", fun_pen);
  document.getElementById("but_res_pen").addEventListener("click", fun_pen);
    function fun_pen() {
    var frase = document.getElementById("res_pen").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_pen").value = frase;
    if (document.getElementById("res_pen").value == "") {
      $('#but_res_pen').addClass('hidden');
      $('#p_pen').addClass('hidden');
      $('#div_error_pen').removeClass('hidden');
    } else {
      $('#div_pen').slideUp();
      $('#div_pen').addClass('hidden');
      $('#pregunta_pen_footer').addClass('hidden');
      $('#div_p1').slideDown();
      $('#div_p1').removeClass('hidden');
      $('#pregunta_p1_footer').removeClass('hidden');
      $('#div_error_p1').addClass('hidden');
      document.getElementById("res_p1").focus();
    }
  }

  $(document).ready(function(){ 

    $('#res_pen').on('input', function() { 
      var frase = document.getElementById("res_pen").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      document.getElementById("res_pen").value = frase;
      if (document.getElementById("res_pen").value == "") {
      $('#but_res_pen').addClass('hidden');
      $('#p_pen').addClass('hidden');
      $('#div_error_pen').removeClass('hidden');
      } else {
       $('#but_res_pen').removeClass('hidden');
       $('#p_pen').removeClass('hidden');
       $('#div_error_pen').addClass('hidden');
      }
    }); 
  }); 
</script>

