<script type="text/javascript">
  var input = document.getElementById("res_pen");
      input.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("but_res_pen").click();
      }
    });

  document.getElementById("pregunta_pregunta_0_pen_boton").addEventListener("click", fun_pen_dev);
