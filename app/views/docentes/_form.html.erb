  var input = document.getElementById("res_p35");
      input.addEventListener("keyup", function(event) {
      if (event.keyCode === 13) {
        event.preventDefault();
        document.getElementById("but_res_p35").click();
      }
    });

  document.getElementById("pregunta_p34_p35_boton").addEventListener("click", fun_p35_dev);
  function fun_p35_dev() {
      $('#div_p34').slideDown();
      $('#div_p34').removeClass('hidden');
      $('#pregunta_p34_footer').removeClass('hidden');
      $('#div_p35').slideUp();
      $('#div_p35').addClass('hidden');
      $('#pregunta_p35_footer').addClass('hidden');
      $('#div_error_p35').addClass('hidden');
  }

  document.getElementById("pregunta_ultima_p35_boton").addEventListener("click", fun_p35);
  document.getElementById("but_res_p35").addEventListener("click", fun_p35);
    function fun_p35() {
    var frase = document.getElementById("res_p35").value; 
      frase = frase.replace("#"," ");
      frase = frase.replace("%"," ");
      frase = frase.replace("&"," ");
      frase = frase.replace("$"," ");
      frase = frase.replace("  "," ");
      document.getElementById("res_p35").value = frase;
    if (document.getElementById("res_p35").value == "") {
      $('#but_res_p35').addClass('hidden');
      $('#p_p35').addClass('hidden');
      $('#div_error_p35').removeClass('hidden');
    } else {
      $('#div_p35').slideUp();
      $('#div_p35').addClass('hidden');
      $('#pregunta_p35_footer').addClass('hidden');
      $('#div_ultima').slideDown();
      $('#div_ultima').removeClass('hidden');
      $('#pregunta_ultima_footer').removeClass('hidden');
      $('#div_error_ultima').addClass('hidden');
      document.getElementById("res_ultima").focus();
    }
  }

  $(document).ready(function(){ 

